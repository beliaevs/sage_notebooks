   "source": [
    "def print_approx(x, n):\n",
    "    a = continued_fraction(x)\n",
    "    return [(i, a.convergent(i), N(x - a.convergent(i), digits=n)) for i in range(n)]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "5ef2cb2b",
   "metadata": {},
   "outputs": [],
